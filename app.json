{
  "expo": {
    "name": "FlashCardsIO",
    "slug": "FlashCardsIO",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/notifIcon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ronaldbarrera.flashcardsio",
      "supportsTablet": true
    },
    "android": {
      "package": "com.ronaldbarrera.flashcardsio"
    }
  },
  "name": "FlashCardsIO"
}
